<launch>
	<arg name="export" default="false" />
	
	<include file="$(find ssl_robot_affw)/launch/trajectory_base.launch">
		<arg name="export" value="$(arg export)" />
	</include>
		
	<param name="use_sim_time" value="false" />
	<param name="robot_id" value="0" />
	<param name="robot_team_color" value="Y" />
	<param name="server_name" value="192.168.20.210" />
    <node name="sumatra_wf_receiver" pkg="ssl_robot_vision" type="sumatra_wf_receiver" output="screen" />
    
    <node name="vel_xyw2SharedRadio" pkg="ssl_robot_comm" type="vel_xyw2SharedRadio" output="screen" />
</launch>
