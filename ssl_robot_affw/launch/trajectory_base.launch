<launch>
	<node name="affw_traj" pkg="affw_ctrl" type="affw_traj" required="true" output="screen" args="/tmp/traj.csv" />
    		
	<param name="dataFolder" value="/tmp/ssl_robot_data" />
		
	<arg name="export" default="false" />
	<group if="$(arg export)">
		<node name="affw_export" pkg="affw_ctrl" type="affw_export" output="screen" />
		<node name="ssl_robot_export" pkg="ssl_robot_affw" type="ssl_robot_export" output="screen" />
	</group>
</launch>